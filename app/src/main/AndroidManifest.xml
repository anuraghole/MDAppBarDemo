<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anuraghole.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeMaterial">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".toolbar.ToolbarActivity" />
        <activity android:name=".toolbar.ToolbarStandAloneActivity" />
        <activity android:name=".toolbar.ToolbarAsActionBarActivity" />
        <activity android:name=".toolbar.ToolbarContextualMenuActivity" />
        <activity android:name=".snackbar.SnackbarActivity" />
        <activity android:name=".animation.touch_feedback.TouchFeedbackAnimationActivity" />
        <activity android:name=".animation.TouchFeedbackAnimationActivity" />
        <activity android:name=".animation.RippleEffectActivity" />
        <activity android:name=".animation.TransitionsActivity" />
        <activity android:name=".animation.FullTransitionActivity" />
        <activity android:name=".app_bar_collapsing_toolbar.AppBarLayoutActivity" />
        <activity android:name=".app_bar_collapsing_toolbar.CollapsingToolbarLayoutActivity"></activity>
    </application>

</manifest>